---
import BackButton from "./BackButton.astro";

interface Props {
  type: "blog" | "vipNewsletter";
}
const { type } = Astro.props;
const { path } = Astro.params;
---

<nav class="w-full top-0 z-10">
  <div class="container px-4 sm:px-8 md:px-14 mx-auto flex">
    {path !== "/" && <BackButton type={type} />}
    <div class="grow"></div>
  </div>
</nav>
